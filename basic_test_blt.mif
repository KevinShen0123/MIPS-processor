-- null
DEPTH = 4096;
WIDTH = 32;

ADDRESS_RADIX = DEC;
DATA_RADIX = BIN;

CONTENT
BEGIN
    -- nop
0000 : 00000000000000000000000000000000;
    -- addi $1, $0, 1  # pc1, r1 = 1
0001 : 00101000010000000000000000000001;
    -- addi $2, $0, 2  # pc2, r2 = 2
0002 : 00101000100000000000000000000010;
    -- blt $1, $2, 1 # pc3, if ($1 < $2) PC = PC + 1 + 1
0003 : 00110000010001000000000000000001;
    -- addi $9, $0, 9 # pc4, r3 = 9
0004 : 00101010010000000000000000001001;
    -- addi $3, $0, 3 # pc5, r3 = 3
0005 : 00101000110000000000000000000011;
    -- blt $1, $1, 1 # pc6, if ($1 < $1) PC = PC + 1 + 1
0006 : 00110000010000100000000000000001;
    -- addi $4, $0, 1 # pc7, r3 = 1
0007 : 00101001000000000000000000000001;
    -- addi $5, $0, 5 # pc8, r3 = 5
0008 : 00101001010000000000000000000101;
    -- blt $9, $4, -6 # pc9, if ($4 < $5) PC = PC + 1 -6
0009 : 00110010010010011111111111111010;
[0010 .. 4095] : 00000000000000000000000000000000;
END;
